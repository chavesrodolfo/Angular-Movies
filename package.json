    "@angular/forms": "^5.2.7",
    "@angular/http": "^5.2.7",
    "@angular/material": "^5.2.3",
    "@angular/platform-browser": "^5.2.7",
    "@angular/platform-browser-dynamic": "^5.2.7",
    "@angular/router": "^5.2.7",
    "@angular/service-worker": "^5.2.7",
    "angularfire2": "^5.0.0-rc.6",
    "core-js": "^2.5.3",
    "firebase": "^4.10.1",
    "hammerjs": "^2.0.8",
    "ngx-swiper-wrapper": "5.2.1",
    "node-sass": "^4.7.2",
