{
  "name": "angular-movies",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "sw": "sw-precache --root=dist --config=sw-precache-config.js",
    "build:local": "ng build --prod && npm run sw && cd dist && live-server --port=4200 --host=localhost --entry-file=/index.html",
    "build:prod": "ng build --prod && npm run sw && firebase deploy",
    "static-serve": "cd dist && live-server --port=4200 --host=localhost --entry-file=/index.html"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.7",
